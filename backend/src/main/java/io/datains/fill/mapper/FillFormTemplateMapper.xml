<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.datains.fill.mapper.FillFormTemplateMapper">
    <select id="getById" resultType="io.datains.fill.entry.FillFormTemplate">
        SELECT *
        FROM fill_form_template
        WHERE id = #{id}
          AND is_delete = 0
    </select>
</mapper>
